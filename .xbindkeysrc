"amixer -q set Master 5%+ unmute"
  XF86AudioRaiseVolume

"amixer -q set Master 5%- unmute"
  XF86AudioLowerVolume

# "amixer -q set Master toggle"
"notify-send mute"
  XF86AudioMute

"playerctl play-pause"
  Alt + p

"xbacklight -inc 10"
  XF86MonBrightnessUp

"xbacklight -dec 10"
  XF86MonBrightnessDown

"rofi -show drun"
  Control + space

"rofi-pass"
  Mod4 + p

"screenshot.sh"
  Mod4 + Print

"display.sh"
  Mod4 + d

"lock.sh"
  Mod4 + Shift + z
