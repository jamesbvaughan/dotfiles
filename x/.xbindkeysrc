"amixer -q set Master 5%+ unmute"
  XF86AudioRaiseVolume

"amixer -q set Master 5%- unmute"
  XF86AudioLowerVolume

"amixer -q set Master toggle"
  XF86AudioMute

"playerctl play-pause"
  Mod4 + Shift + p

"playerctl next"
  Mod4 + Shift + period

"xbacklight -inc 10"
  XF86MonBrightnessUp

"xbacklight -dec 10"
  XF86MonBrightnessDown

"rofi -show combi"
  Control + space

"rofi-pass"
  Mod4 + p

"="
  Mod4 + u

"pdf-opener.sh"
  Mod4 + f

"screenshot.sh"
  Mod4 + Print

"display.sh"
  Mod4 + d

"lock.sh"
  Mod4 + Shift + z
